<launch>

  <param name="lunar_env" textfile="$(find distributed_planning)/data/$(arg lunar_env)" />

  <node pkg="distributed_planning" type="agent_dmarrt.py" name="$(anon agent-1)" />
  <node pkg="distributed_planning" type="agent_dmarrt.py" name="$(anon agent-2)" />
  <node pkg="distributed_planning" type="agent_dmarrt.py" name="$(anon agent-3)" />
  <node pkg="distributed_planning" type="agent_dmarrt.py" name="$(anon agent-4)" />
  <node pkg="distributed_planning" type="agent_dmarrt.py" name="$(anon agent-5)" >
    <param name="has_token" type="bool" value="true" />
  </node>

</launch>
